{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "node": true,
    "worker": true,
    "mocha": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off"
  }
}
